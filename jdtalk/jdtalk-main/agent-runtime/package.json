{
  "name": "jdcredvip-codex-realtime-agent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:agent": "cross-env NODE_ENV=development tsx agents/realtime-agent.ts",
    "build": "tsc -p tsconfig.json",
    "start:agent": "node dist/agents/realtime-agent.js",
    "pm2:start": "pm2 start pm2-ecosystem.config.cjs --only codex-realtime",
    "pm2:restart": "pm2 restart codex-realtime",
    "pm2:logs": "pm2 logs codex-realtime --lines 200"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "dotenv": "^16.4.5",
    "openai": "^6.7.0",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "@types/node": "20.16.11",
    "cross-env": "^7.0.3",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  }
}
